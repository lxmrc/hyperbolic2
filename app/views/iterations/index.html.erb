<div class="row">
  <div class="col-sm-6">
    <h1>Iterations</h1>
  </div>

  <div class="col-sm-6 text-end">
  <%= link_to new_iteration_path, class: 'btn btn-primary' do %>
    Add New Iteration
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Code</th>
            <th>Exercise</th>
            <th>User</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @iterations.each do |iteration| %>
        <%= content_tag :tr, id: dom_id(iteration), class: dom_class(iteration) do %>
          

                      <td><%= iteration.code %></td>
                      <td><%= iteration.exercise %></td>
                      <td><%= iteration.user %></td>
          
          
            <td><%= link_to 'Show', iteration %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
