<div class="page-header">
  <%= link_to exercises_path, class: 'btn btn-default' do %>
    All Exercises
  <% end %>
  <%= link_to edit_exercise_path(@exercise), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show exercise</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @exercise.name %></dd>
  <dt>Description:</dt>
  <dd><%= @exercise.description %></dd>
  <dt>Tests:</dt>
  <dd><%= @exercise.tests %></dd>
</dl>
